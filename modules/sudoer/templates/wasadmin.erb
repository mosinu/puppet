Cmnd_Alias WASADMIN_ADMIN_CMDS = /bin/su wasadmin , /bin/su - wasadmin , /usr/bin/xauth * , /opt/IBM/InstallationManager/eclipse/* , /opt/IBM/ibmim/* , /opt/IBM/WebSphere/Toolbox/WCT/* , /opt/IBM/HTTPServer/bin/*, /bin/netstat *
Cmnd_Alias WASADMIN_APP_ADMIN_CMDS = /bin/sh 
Cmnd_Alias WASADMIN_APP_CMDS = /bin/su wasadmin , /bin/su - wasadmin , /usr/bin/xauth * , /opt/IBM/InstallationManager/eclipse/* , /opt/IBM/ibmim/* , /opt/IBM/WebSphere/Toolbox/WCT/* , /opt/IBM/HTTPServer/bin/*, /bin/netstat *

